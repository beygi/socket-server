{
    "name": "SocketServer",
    "version": "1.0.0",
    "main": "dist/server.js",
    "scripts": {
        "build": "tsc",
        "watch-ts": "tsc -w",
        "watch-node": "nodemon dist/server.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"yarn run watch-ts\" \"yarn run watch-node\"",
        "test": "mocha -r ts-node/register src/**/*.spec.ts",
        "production": "nodejs dist/server.js"
    },
    "dependencies": {
        "@types/express": "^4.17.0",
        "@types/socket.io": "^2.1.2",
        "chalk": "^2.4.2",
        "concurrently": "^4.1.1",
        "express": "^4.17.3",
        "install": "^0.13.0",
        "jsonwebtoken": "^8.5.1",
        "nodemon": "^1.19.1",
        "npm": "^6.10.0",
        "redis": "^2.8.0",
        "socket.io": "^2.2.0",
        "socketio-auth": "^0.1.1",
        "string-align": "^0.2.0"
    },
    "devDependencies": {
        "tslint": "^5.18.0"
    }
}
